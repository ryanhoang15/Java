public int[] fix23(int[] nums) {
  int[] n = new int[nums.length];
  if(nums[0] == 2 && nums[1] == 3){
    n[0] = nums[0];
    n[1] = 0;
    n[2] = nums[2];
  }
  else if(nums[1] == 2 && nums[2] == 3){
    n[0] = nums[0];
    n[1] = nums[1];
    n[2] = 0;
  }
  else{
    n[0] = nums[0];
    n[1] = nums[1];
    n[2] = nums[2];
  }
  return n;
}
